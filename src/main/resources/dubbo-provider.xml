<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
    xmlns:util="http://www.springframework.org/schema/util"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://code.alibabatech.com/schema/dubbo       
            http://code.alibabatech.com/schema/dubbo/dubbo.xsd
            http://www.springframework.org/schema/util
            http://www.springframework.org/schema/util/spring-util-3.2.xsd">
     
     <!-- 
    <util:properties id="zcparams" location="classpath:params.properties"></util:properties> 
     -->
    <dubbo:application name="provider" owner="programmer" organization="dubbox" />
 
    <dubbo:registry address="zookeeper://192.168.1.201:2181?backup=192.168.1.202:2181,192.168.1.203:2181" />
 
	<dubbo:annotation package="com.fgh.service"/>
	
	<!--kyro实现序列化 serialization="kyro"-->
	<dubbo:protocol name="dubbo"  serialization="kryo"
		optimizer="com.fgh.util.SerializationOptimizerImpl"></dubbo:protocol> 
	
	<!-- rest -->
	<dubbo:protocol name="rest" server="tomcat" port="8888" contextpath="provider" accepts="500" ></dubbo:protocol>
	
	
	
</beans>